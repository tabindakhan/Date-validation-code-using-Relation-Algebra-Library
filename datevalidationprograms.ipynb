   "source": [
    "import re\n",
    "pat=\"^[0-9+]{4}-[0][1-9]|[1][0-2]-[0-9+]{1}[1-9+]{1}|[0-9+]{1}[1-9+]{1}-[0][1-9]|[1][0-2]-[0-9+]{4}\"\n",
    "da = input(\"Enter Date in Format of D-M-Y or Y-M-D:\")\n",
    "if(re.search(pat,da)):\n",
    "    print(\"valid date\")\n",
    "else:\n",
    "     print(\"invalid date\")"
